{
  "name": "linkedin-crawler",
  "version": "1.0.0",
  "description": "TypeScript style guide, formatter, and linter. https://github.com/google/gts",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon build/src/index.js\"",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watchAll",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "author": "Bastien DURAN",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "concurrently": "^5.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "gts": "^2.0.2"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bastienduran/typescriptAndNode.git"
  },
  "keywords": [
    "Typescript",
    "Javascript",
    "Jest",
    "Crawler",
    "Prettier",
    "Eslint"
  ],
  "bugs": {
    "url": "https://github.com/bastienduran/typescriptAndNode/issues"
  },
  "homepage": "https://github.com/bastienduran/typescriptAndNode#readme"
}
